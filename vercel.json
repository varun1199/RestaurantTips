{
  "version": 2,
  "builds": [
    { "src": "package.json", "use": "@vercel/node" },
    { "src": "api/*.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api", "dest": "/api/adapter.js" },
    { "src": "/api/(.*)", "dest": "/api/adapter.js" },
    { "src": "/static", "dest": "/api/static.js" },
    { "src": "/assets/(.*)", "dest": "/dist/assets/$1" },
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/api/static.js" }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}