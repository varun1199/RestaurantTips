{
  "version": 2,
  "builds": [
    { "src": "api/*.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api", "dest": "/api/index.js" },
    { "src": "/api/hello", "dest": "/api/hello.js" },
    { "src": "/(.*)", "dest": "/api/static.js" }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}