{
  "version": 3,
  "routes": [
    {
      "src": "^/api/?$",
      "dest": "/api/index.js"
    },
    {
      "src": "^/api/health/?$",
      "dest": "/api/health.js"
    },
    {
      "src": "^/api/hello/?$",
      "dest": "/api/hello.js"
    },
    {
      "src": "^/api/db-config/?$",
      "dest": "/api/db-config.js"
    },
    {
      "src": "^/api/neon-test/?$",
      "dest": "/api/neon-test.js"
    },
    {
      "src": "^/api/status/?$",
      "dest": "/api/status.js"
    },
    {
      "src": "^/status/?$",
      "dest": "/api/status.js"
    },
    {
      "src": "^/fallback/?$",
      "dest": "/api/fallback.js"
    },
    {
      "src": "^/(?!api|status|fallback)(.*)",
      "dest": "/api/serve.js"
    }
  ]
}